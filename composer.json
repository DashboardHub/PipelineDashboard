{
    "name": "EddieJaoude/SymfonyQuickstart",
    "license": "MIT",
    "type": "project",
    "description": "Symfony quickstart & best practice tools - Behat 3, PHPSpec 2, PHPUnit 4 & Twitter Bootstrap 3",
    "autoload": {
        "psr-0": { "": "src/", "SymfonyStandard": "app/" }
    },
    "require": {
        "php": "~5.5",
        "ext-mbstring": "*",
        "symfony/symfony": "2.6.*",
        "doctrine/orm": "~2.2,>=2.2.3",
        "doctrine/doctrine-bundle": "~1.2",
        "twig/extensions": "~1.0",
        "symfony/assetic-bundle": "~2.3",
        "symfony/swiftmailer-bundle": "~2.3",
        "symfony/monolog-bundle": "~2.4",
        "sensio/distribution-bundle": "~3.0",
        "sensio/framework-extra-bundle": "~3.0",
        "incenteev/composer-parameter-handler": "~2.0",
        "braincrafted/bootstrap-bundle": "~2.0",
        "knplabs/knp-menu-bundle": "~2.0",
        "jms/i18n-routing-bundle": "1.1.*",
        "symfony/yaml": "2.6.*",
        "twbs/bootstrap": "3.3.*",
        "jquery/jquery":  "1.11.1",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "eddiejaoude/eddie-jaoude-symfony-translation-twig-collection-bundle": "0.1.*",
        "eddiejaoude/dev-helper-cmds": "dev-master",
        "doctrine/doctrine-fixtures-bundle": "2.2.*",
        "knplabs/knp-time-bundle": "~1.3",
        "tedivm/stash-bundle": "0.4.*",
        "guzzlehttp/guzzle": "5.*"
    },
    "require-dev": {
        "heroku/heroku-buildpack-php": "*",
        "sensio/generator-bundle": "2.5.*",
        "behat/behat":  "3.0.*",
        "behat/symfony2-extension": "~2.0@dev",
        "behat/mink-extension": "~2.0@dev",
        "behat/mink-browserkit-driver": "~1.1@dev",
        "behat/mink-goutte-driver": "*@dev",
        "behat/mink-selenium2-driver": "1.*",
        "chartinger/behat-twig-report-extension": "*@dev",
        "phpunit/phpunit": "4.*@stable",
        "phpspec/phpspec": "~2.0",
        "codegyre/robo": "*",
        "henrikbjorn/phpspec-code-coverage": "~0.2"
    },
    "scripts": {
        "post-root-package-install": [
            "SymfonyStandard\\Composer::hookRootPackageInstall"
        ],
        "post-install-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ],
        "post-update-cmd": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::removeSymfonyStandardFiles",
            "Braincrafted\\Bundle\\BootstrapBundle\\Composer\\ScriptHandler::install",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache"
        ]
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-web-dir": "web",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "2.5-dev"
        }
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jquery/jquery",
                "version": "1.11.1",
                "dist": {
                    "url": "http://code.jquery.com/jquery-1.11.1.js",
                    "type": "file"
                }
            }
        }
    ]
}
