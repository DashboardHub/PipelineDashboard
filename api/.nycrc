{
    "cache": true,
    "check-coverage": true,
    "lines": 75,
    "statements": 75,
    "functions": 75,
    "branches": 75,
    "include": ["src/**/*.js"],
    "reporter": ["lcov", "text-summary"],
    "report-dir": ".coverage",
    "temp-directory": ".coverage/.nyc_output",
    "watermarks": {
        "lines": [75, 90],
        "statements": [75, 90],
        "functions": [75, 90],
        "branches": [75, 90]
    }
}
