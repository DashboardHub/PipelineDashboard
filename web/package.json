{
  "name": "dashboardhub-pipelinedashboard",
  "version": "0.9.0",
  "private": true,
  "description": "Your Delivery Pipeline overview in One Place",
  "keywords": [
    "dashboardhub",
    "pipelinedashboard"
  ],
  "scripts": {
    "start": "ng serve",
    "build:test": "ng build --prod --aot --env=test",
    "build:prod": "ng build --prod --aot --env=prod",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reinstall": "rm -rf node_modules tmp deploy dist && yarn install && npm rebuild node-sass",
    "reinstall:latest": "rm -rf node_modules tmp deploy dist && npm install",
    "postinstall": "webdriver-manager update && ./scripts/protractor.sh",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "test:watch": "ng test --code-coverage"
  },
  "engines": {
    "node": ">6.12.0",
    "npm": ">3"
  },
  "repository": "git@github.com:DashboardHub/PipelineDashboard.git",
  "license": "MIT",
  "author": "DashboardHub",
  "dependencies": {
    "@angular/animations": "^6.0.0-rc.5",
    "@angular/cdk": "^6.0.0-rc.5",
    "@angular/common": "^6.0.0-rc.5",
    "@angular/compiler": "^6.0.0-rc.5",
    "@angular/core": "^6.0.0-rc.5",
    "@angular/forms": "^6.0.0-rc.5",
    "@angular/http": "^6.0.0-rc.5",
    "@angular/material": "^6.0.0-rc.5",
    "@angular/platform-browser": "^6.0.0-rc.5",
    "@angular/platform-browser-dynamic": "^6.0.0-rc.5",
    "@angular/platform-server": "^6.0.0-rc.5",
    "@angular/router": "^6.0.0-rc.5",
    "@auth0/angular-jwt": "^2.0.0",
    "@covalent/core": "^2.0.0-beta.1",
    "@covalent/dynamic-forms": "^2.0.0-beta.1",
    "@covalent/highlight": "^2.0.0-beta.1",
    "@covalent/http": "^2.0.0-beta.1",
    "@covalent/markdown": "^2.0.0-beta.1",
    "@swimlane/ngx-charts": "^7.3.0",
    "auth0-js": "^9.5.0",
    "core-js": "^2.4.1",
    "d3": "^4.9.0",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.10.0",
    "rxjs": "^6.0.0-rc.0",
    "rxjs-compat": "^6.0.0-rc.0",
    "showdown": "1.6.4",
    "zone.js": "^0.8.20"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^6.0.0-rc.5",
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "2.53.36",
    "codelyzer": "~4.0.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.6.0",
    "karma-spec-reporter": "^0.0.31",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "~5.1.0",
    "ts-node": "3.1.0",
    "tslint": "^5.2.0",
    "typescript": "~2.7.2"
  },
  "optionalDependencies": {
    "@covalent/data": "0.7.0"
  }
}
