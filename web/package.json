{
  "name": "dashboardhub-pipelinedashboard",
  "version": "0.11.6",
  "private": true,
  "description": "Your Delivery Pipeline overview in One Place",
  "keywords": [
    "dashboardhub",
    "pipelinedashboard",
    "devops"
  ],
  "scripts": {
    "start": "ng serve --configuration=local",
    "build:local": "ng build --prod --aot --configuration=local",
    "build:prod": "ng build --prod --aot --configuration=production",
    "lint": "ng lint",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm install && npm rebuild node-sass",
    "reinstall:latest": "rm -rf node_modules tmp deploy dist && npm install",
    "start:test": "npx serve -s -p 8080 dist",
    "test": "npm run lint && npm run build:prod && concurrently -r -k -s first \"npm run start:test\" \"npm run cypress:run\"",
    "cypress:run": "npx cypress run",
    "cypress:open": "npx cypress open"
  },
  "engines": {
    "node": ">10.15.2",
    "npm": ">6.4.1"
  },
  "repository": "git@github.com:DashboardHub/PipelineDashboard.git",
  "license": "MIT",
  "author": "DashboardHub",
  "cypress-cucumber-preprocessor": {
    "commonPath": "common",
    "step_definitions": "test/step_definitions"
  },
  "dependencies": {
    "@angular/animations": "7.2.15",
    "@angular/cdk": "7.3.7",
    "@angular/common": "7.2.15",
    "@angular/compiler": "7.2.15",
    "@angular/core": "7.2.15",
    "@angular/fire": "^5.1.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "7.2.15",
    "@angular/http": "7.2.15",
    "@angular/material": "7.3.7",
    "@angular/platform-browser": "7.2.15",
    "@angular/platform-browser-dynamic": "7.2.15",
    "@angular/platform-server": "7.2.15",
    "@angular/router": "7.2.15",
    "classlist.js": "1.1.20150312",
    "core-js": "2.5.7",
    "d3": "5.7.0",
    "firebase": "^5.5.9",
    "hammerjs": "2.0.8",
    "highlight.js": "9.13.1",
    "marked": "^0.7.0",
    "ngx-device-detector": "^1.3.4",
    "normalize.css": "8.0.0",
    "rxjs": "6.5.2",
    "tslib": "^1.9.0",
    "tslint-etc": "^1.6.0",
    "uuid": "^3.3.2",
    "web-animations-js": "2.3.1",
    "zone.js": "0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "7.3.9",
    "@angular/compiler-cli": "7.2.15",
    "@types/hammerjs": "2.0.36",
    "@types/jasmine": "2.8.9",
    "@types/node": "10.12.0",
    "codelyzer": "4.5.0",
    "concurrently": "^4.1.0",
    "cypress": "^3.4.1",
    "cypress-cucumber-preprocessor": "^1.13.1",
    "serve": "^11.1.0",
    "ts-node": "7.0.1",
    "tslint": "5.11.0",
    "typescript": "3.2.4"
  }
}
