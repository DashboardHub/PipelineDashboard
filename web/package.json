{
    "name": "dashboardhub-pipelinedashboard",
    "version": "0.9.0",
    "private": true,
    "description": "Your Delivery Pipeline overview in One Place",
    "keywords": [
        "dashboardhub",
        "pipelinedashboard"
    ],
    "scripts": {
        "start": "ng serve",
        "e2e": "ng e2e",
        "lint": "ng lint",
        "reinstall": "rm -rf node_modules tmp deploy dist && yarn install && npm rebuild node-sass",
        "reinstall:latest": "rm -rf node_modules tmp deploy dist && npm install",
        "postinstall": "webdriver-manager update && ./scripts/protractor.sh",
        "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
        "test": "ng test --code-coverage --single-run",
        "test:watch": "ng test --code-coverage"
    },
    "engines": {
        "node": ">6.12.0",
        "npm": ">3"
    },
    "repository": "git@github.com:DashboardHub/PipelineDashboard.git",
    "license": "MIT",
    "author": "DashboardHub",
    "dependencies": {
        "@angular/animations": "7.0.1",
        "@angular/cdk": "7.0.2",
        "@angular/common": "7.0.1",
        "@angular/compiler": "7.0.1",
        "@angular/core": "7.0.1",
        "@angular/fire": "^5.1.0",
        "@angular/flex-layout": "7.0.0-beta.19",
        "@angular/forms": "7.0.1",
        "@angular/http": "7.0.1",
        "@angular/material": "7.0.2",
        "@angular/platform-browser": "7.0.1",
        "@angular/platform-browser-dynamic": "7.0.1",
        "@angular/platform-server": "7.0.1",
        "@angular/router": "7.0.1",
        "@swimlane/ngx-charts": "9.0.0",
        "classlist.js": "1.1.20150312",
        "core-js": "2.5.7",
        "d3": "5.7.0",
        "firebase": "^5.5.9",
        "hammerjs": "2.0.8",
        "highlight.js": "9.13.1",
        "marked": "0.5.1",
        "normalize.css": "8.0.0",
        "rxjs": "6.3.3",
        "web-animations-js": "2.3.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.10.3",
        "@angular/cli": "7.0.3",
        "@angular/compiler-cli": "7.0.1",
        "@types/hammerjs": "2.0.36",
        "@types/jasmine": "2.8.9",
        "@types/node": "10.12.0",
        "@types/selenium-webdriver": "3.0.12",
        "codelyzer": "4.5.0",
