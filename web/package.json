{
  "name": "dashboardhub-pipelinedashboard",
  "version": "0.9.0",
  "private": true,
  "description": "Your Delivery Pipeline overview in One Place",
  "keywords": [
    "dashboardhub",
    "pipelinedashboard",
    "devops"
  ],
  "scripts": {
    "start": "ng serve",
    "build:dev": "ng build --prod --aot",
    "build:test": "ng build --prod --aot --configuration=test",
    "build:prod": "ng build --prod --aot --configuration=production",
    "lint": "ng lint",
    "reinstall": "rm -rf node_modules tmp deploy dist && npm install && npm rebuild node-sass",
    "reinstall:latest": "rm -rf node_modules tmp deploy dist && npm install",
    "test": "npm run lint && npm run build:dev && concurrently -k -s first \"npm start\" \"sleep 15 && npm run e2e:ci\""
  },
  "engines": {
    "node": ">6.12.0",
    "npm": ">3"
  },
  "repository": "git@github.com:DashboardHub/PipelineDashboard.git",
  "license": "MIT",
  "author": "DashboardHub",
  "dependencies": {
    "@angular/animations": "8.0.0",
    "@angular/cdk": "8.0.0",
    "@angular/common": "8.0.0",
    "@angular/compiler": "8.0.0",
    "@angular/core": "8.0.0",
    "@angular/fire": "^5.1.0",
    "@angular/flex-layout": "7.0.0-beta.19",
    "@angular/forms": "8.0.0",
    "@angular/http": "7.2.15",
    "@angular/material": "8.0.0",
    "@angular/platform-browser": "8.0.0",
    "@angular/platform-browser-dynamic": "8.0.0",
    "@angular/platform-server": "8.0.0",
    "@angular/router": "8.0.0",
    "classlist.js": "1.1.20150312",
    "core-js": "3.1.3",
    "d3": "5.9.2",
    "firebase": "^6.1.0",
    "hammerjs": "2.0.8",
    "highlight.js": "9.15.8",
    "marked": "^0.6.2",
    "ngx-device-detector": "^1.3.4",
    "normalize.css": "8.0.1",
    "rxjs": "6.5.2",
    "tslib": "^1.9.0",
    "uuid": "^3.3.2",
    "web-animations-js": "2.3.1",
    "zone.js": "0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.1",
    "@angular/cli": "8.0.1",
    "@angular/compiler-cli": "8.0.0",
    "@types/hammerjs": "2.0.36",
    "@types/jasmine": "3.3.13",
    "@types/node": "12.0.5",
    "codelyzer": "5.1.0",
    "concurrently": "^4.1.0",
    "ts-node": "8.2.0",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  }
}
