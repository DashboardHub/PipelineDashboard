{
    "name": "dashboardhub-pipelinedashboard",
    "version": "0.9.0",
    "private": true,
    "description": "Your Delivery Pipeline overview in One Place",
    "keywords": [
        "dashboardhub",
        "pipelinedashboard",
        "devops"
    ],
    "scripts": {
        "start": "ng serve",
        "build:dev": "ng build --prod --aot",
        "build:test": "ng build --prod --aot --configuration=test",
        "build:prod": "ng build --prod --aot --configuration=production",
        "e2e": "cypress open",
        "e2e:ci": "cypress run",
        "lint": "ng lint",
        "reinstall": "rm -rf node_modules tmp deploy dist && npm install && npm rebuild node-sass",
        "reinstall:latest": "rm -rf node_modules tmp deploy dist && npm install",
        "test": "npm run lint && npm run build:dev && concurrently -k -s first \"npm start\" \"sleep 15 && npm run e2e:ci\""
    },
    "engines": {
        "node": ">6.12.0",
        "npm": ">3"
    },
    "repository": "git@github.com:DashboardHub/PipelineDashboard.git",
    "license": "MIT",
    "author": "DashboardHub",
    "dependencies": {
        "@angular/animations": "7.0.1",
        "@angular/cdk": "7.0.2",
        "@angular/common": "7.0.1",
        "@angular/compiler": "7.0.1",
        "@angular/core": "7.0.1",
        "@angular/fire": "^5.1.0",
        "@angular/flex-layout": "7.0.0-beta.19",
        "@angular/forms": "7.0.1",
        "@angular/http": "7.0.1",
        "@angular/material": "7.0.2",
        "@angular/platform-browser": "7.0.1",
        "@angular/platform-browser-dynamic": "7.0.1",
        "@angular/platform-server": "7.0.1",
        "@angular/router": "7.0.1",
        "@swimlane/ngx-charts": "9.0.0",
        "classlist.js": "1.1.20150312",
        "core-js": "2.5.7",
        "d3": "5.7.0",
        "firebase": "^5.5.9",
        "hammerjs": "2.0.8",
        "highlight.js": "9.13.1",
        "marked": "0.5.1",
        "normalize.css": "8.0.0",
        "rxfire": "^3.2.0",
        "rxjs": "6.3.3",
        "web-animations-js": "2.3.1",
        "zone.js": "0.8.26"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "0.10.3",
        "@angular/cli": "7.0.3",
        "@angular/compiler-cli": "7.0.1",
        "@types/hammerjs": "2.0.36",
        "@types/jasmine": "2.8.9",
        "@types/node": "10.12.0",
        "codelyzer": "4.5.0",
        "concurrently": "^4.1.0",
        "cypress": "^3.1.3",
        "ts-node": "7.0.1",
        "tslint": "5.11.0",
        "typescript": "3.1.3"
    }
}
